<ul>
  <% if logged_in? -%>
    <li><%= I18n.t("tog_core.views.shared.header_options.greeting")%> <%= current_user.profile.full_name %></li>
    <li><%= link_to I18n.t("interface.layout.header.home"), member_dashboard_path -%></li>
	<li><%= link_to I18n.t("interface.layout.header.profile"), member_profile_path(current_user.profile) -%></li>
	<li><%= link_to I18n.t("interface.layout.header.schoolarly"), "/" -%></li>
  	<% if current_user.admin? %>
      	<li><%= link_to I18n.t("interface.layout.header.schools"), "/admin/groups?type=schools" -%></li>
  	<% end %>	
    <li><%= link_to I18n.t("tog_core.views.shared.header_options.logout"), logout_path -%></li>
  <% else -%>
    <li><%= link_to I18n.t("tog_core.views.shared.header_options.login"), login_path -%></li>
  <% end -%>
  <li class="last"><%= link_to "Help", "/help" -%></li>
</ul>
